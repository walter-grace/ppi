{
  "mcpServers": {
    "next-devtools": {
      "command": "npx",
      "args": ["-y", "next-devtools-mcp@latest"]
    },
    "watch-database": {
      "command": "npx",
      "args": [
        "-y",
        "mcp-remote",
        "https://mcp.rapidapi.com",
        "--header",
        "x-api-host: watch-database1.p.rapidapi.com",
        "--header",
        "x-api-key: ${WATCH_DATABASE_API_KEY}"
      ]
    }
  }
}

